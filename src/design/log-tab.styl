@import "~@design"
.log-header
  width :100%
  layout(block,nowrap,block)
  align-items :center
  height: 40px
  font-family: $font-family-regular
  font-size: 13px;
  color: #818D99;
  line-height: @height
.log-item-wrapper
  height: 60px
  width : 100%
  layout(row,nowrap,block)
  font-family: $font-family-regular
  line-height :1
  .default-side
    display :flex
    flex-direction :column
    padding-top :13px
    .up
      font-size: 14px;
      color: #3F454B;
    .down
      padding-top :8px
      font-size: 12px;
      color: #818D99;
    .up-other
      font-family: $font-family-medium
      font-size: 15px;
      color: #1D2023;
      letter-spacing: 0.32px;
      text-align :right
